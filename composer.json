{
    "name": "mphpmaster/laravel-helpers",
    "description": "Laravel hlaCk helpers",
    "type": "library",
    "keywords": [
        "laravel",
        "hlaCk",
        "helpers"
    ],
    "require": {
        "php": ">=8.1",
        "laravel/framework": ">=9",
        "laravel/helpers": "^1.5",
        "mphpmaster/laravel-dev-helpers": "^1",
        "mphpmaster/laravel-guesser-helpers": "^1",
        "mphpmaster/laravel-helpers2": "^1",
        "mphpmaster/laravel-app-helpers": "^1",
        "mphpmaster/laravel-nova-helpers": "^1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "mPhpMaster",
            "email": "59211285+mPhpMaster@users.noreply.github.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "MPhpMaster\\LaravelHelpers\\": ""
        },
        "files": [
            "Helpers/Files.functions.php",
            "Helpers/General.functions.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "MPhpMaster\\LaravelHelpers\\Providers\\HelperProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "version": "3.0.0"
}
