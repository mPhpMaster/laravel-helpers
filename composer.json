{
    "name": "mphpmaster/laravel-helpers",
    "description": "Laravel hlaCk helpers",
    "type": "library",
    "require": {
        "php": ">=7.4",
        "laravel/framework": ">=5.8",
        "laravel/helpers": "^1.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "mPhpMaster",
            "email": "59211285+mPhpMaster@users.noreply.github.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "mPhpMaster\\Support\\": "src/"
        },
        "files": [
            "src/Helpers/Functions.php",
            "src/Helpers/Global.functions.php",
            "src/Helpers/HelpersLoader.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "mPhpMaster\\Support\\Providers\\MPhpMasterHelpersProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "scripts": {
        "helpers": [
            "@php -r \"@unlink('.phpstorm.meta.php');@unlink('_ide_helper.php');@unlink('_ide_helper_models.php');\"",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:model -N",
            "@php artisan ide-helper:eloquent"
        ],
        "c:c": [
            "@php artisan cache:clear",
            "@php artisan route:clear",
            "@php artisan config:clear",
            "@php artisan view:clear"
        ],
        "a:routes": [
            "@php artisan route:list --no-ansi > routes.txt"
        ]
    }
}
